  "type": "library",
  "keywords": [
    "eloquent",
    "filter"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/horizom/eloquent-filter/issues",
    "source": "https://github.com/horizom/eloquent-filter"
  },
  "authors": [
    {
      "name": "Roland Edi",
      "email": "lambirou225@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Horizom\\EloquentFilter\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
